<Page
    x:Class="WarGearCalc.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WarGearCalc"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button Content="Test" HorizontalAlignment="Left" Height="146" Margin="384,155,0,0" VerticalAlignment="Top" Width="167" 
                Click="Test_Button_Click"/>
        <ComboBox HorizontalAlignment="Left" Margin="170,150,0,0" VerticalAlignment="Top" 
                  SelectionChanged="Faction_ComboBox_SelectionChanged" ItemsSource="{x:Bind factions}"/>
    </Grid>
</Page>
